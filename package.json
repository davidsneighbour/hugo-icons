{
  "name": "@dnb-org-libraries/bootstrap-icons",
  "description": "Bootstrap Icons for Hugo",
  "version": "1.6.0",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-org/libraries/issues"
  },
  "devDependencies": {
    "@dnb-org/standard-version-config": "3.4.6",
    "@dnb-org/tools": "^3.4.8"
  },
  "homepage": "https://github.com/dnb-org/libraries/tree/master/bootstrap-icons",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"bootstrap-icons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:major": "standard-version --release-as major -a -t \"bootstrap-icons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:minor": "standard-version --release-as minor -a -t \"bootstrap-icons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  }
}
